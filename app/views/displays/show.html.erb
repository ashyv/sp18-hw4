<h1> Cats </h1>
<% @cats.each do |cat| %>
<p>Name: <%= cat.name %></p>
<% end %>

<h1> Users </h1>
<% @users.each do |user| %>
<p>Name: <%= user.username %></p>
<p>Age: <%= user.age %></p>
<p>Email: <%= user.email %></p>
<% end %>

<h1> Todos </h1>
<% @todos.each do |todo| %>
<p>Task: <%= todo.tasks %></p>
<p>Finished: <%= todo.finished %></p>
<% end %>


